    <!-- AOS Library Script -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.js"></script>

    <script>
        // Initialize AOS
        AOS.init();

        // Back to Top Button Logic
        const topButton = document.querySelector('.BTT-button');

        window.onscroll = function () {
            if (document.body.scrollTop > 100 || document.documentElement.scrollTop > 100) {
                topButton.classList.add('show');
            } else {
                topButton.classList.remove('show');
            }
        };

        function topFunction() {
            document.body.scrollTop = 0; // For Safari
            document.documentElement.scrollTop = 0; // For Chrome, Firefox, IE, and Opera
        }

        // Add the 'show' class to each photo-text container with a slight delay
        window.onload = function() {
            const elements = document.querySelectorAll('.photo-text-container');
            elements.forEach((el, index) => {
                setTimeout(() => {
                    el.classList.add('show');
                }, index * 500);  // Delay increases by 500ms for each element
            });
        };
    </script>

